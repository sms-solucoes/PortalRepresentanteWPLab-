<%
	/**** Pagina de login do site ****/
//	Local cLSite      := "./"   // Site 
	Local cLTitle     := "" 	// Titulo da pagina
	Local cLPagina    := "" 	// Nome da pagina
//	Local cLHeader    := ""
	Local cLMenus     := ""
	Local cLWidgets   := ""
	Local cLWgMeta    := ""
	Local cLCodLogin  := ""
	local cLMVendas   := ""
	local cLVendaMes  := ""
	local cLListVend  := ""
	local cLMFiliais  := ""
	local cLMEstados  := ""
	local cLListaSaldo:= ""
	if type("cSite") <> "U"
		cLSite := cSite
	else
		conout(dtoc(date())+" " + time() + " Portal "+procname(1)+"("+cValToChar(procline(1))+") Faltou cSite")
	Endif
	if type("cTitle") <> "U"
		cLTitle := cTitle
	else
		conout(dtoc(date())+" " + time() + " Portal "+procname(1)+"("+cValToChar(procline(1))+") Faltou cTitle")
	Endif
	if type("cPagina") <> "U"
		cLPagina := cPagina
	else
		conout(dtoc(date())+" " + time() + " Portal "+procname(1)+"("+cValToChar(procline(1))+") Faltou cPagina")
	Endif
	if type("cMenus") <> "U"
		cLMenus := cMenus
	else
		conout(dtoc(date())+" " + time() + " Portal "+procname(1)+"("+cValToChar(procline(1))+") Faltou cMenus")
	Endif
	if type("cWidgets") <> "U"
		cLWidgets := cWidgets
	else
		conout(dtoc(date())+" " + time() + " Portal "+procname(1)+"("+cValToChar(procline(1))+") Faltou cWidgets")
	Endif
	if type ("cCodLogin") <> "U"
		cLCodLogin := cCodLogin
	Endif
	if type("cWgMeta") <> "U"
		cLWgMeta   := cWgMeta
	endif
	if type("cMVendas") <> "U"
		cLMVendas  := cMVendas
	endif
	if type("cVendaMes") <> "U"
		cLVendaMes  := cVendaMes
	endif
%>
<!DOCTYPE html>
<html class="fixed">
<head>
	<!-- Basic -->
	<meta charset="iso-8859-1">
